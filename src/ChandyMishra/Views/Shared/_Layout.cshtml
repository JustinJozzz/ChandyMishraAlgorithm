<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ChandyMisra</title>
    <script src="https://www.gstatic.com/firebasejs/4.6.2/firebase.js"></script>
    <script>
        // Initialize Firebase
        var config = {
            apiKey: "AIzaSyA5AyNbsJR6XM1Njt--3nlcpGPn7LI85R8",
            authDomain: "chandymishra-74bcd.firebaseapp.com",
            databaseURL: "https://chandymishra-74bcd.firebaseio.com",
            projectId: "chandymishra-74bcd",
            storageBucket: "",
